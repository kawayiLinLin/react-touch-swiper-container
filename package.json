{
  "name": "react-touch-swiper-container",
  "version": "1.0.2",
  "description": "A react touch swiper mobile component",
  "main": "./lib/index",
  "module": "./es/index",
  "files": [
    "lib",
    "es"
  ],
  "typings": "./lib/index.d.ts",
  "scripts": {
    "build": "rc-tools run watch",
    "dev": "rc-tools run server",
    "test": "jest",
