    "postpublish": "git push origin master & git push github master & git push github --tag & git push origin --tag"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kawayiLinLin/react-touch-swiper-container.git"
  },
  "homepage": "https://kawayilinlin.github.io/react-touch-swiper-container",
  "config": {
    "port": 8090,
    "entry": [
      "./src/index.tsx"
    ]
  },
  "keywords": [
    "react",
    "components",
    "swiper",
    "mobile"
  ],
  "author": "kawayiLinLin",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-typescript": "^7.16.0",
    "@testing-library/react": "^12.1.2",
    "@types/react": "^17.0.10",
    "@types/react-dom": "^17.0.10",
    "babel-jest": "^27.3.1",
    "jest": "^27.3.1",
    "jest-environment-jsdom": "^27.3.1",
    "rc-tools": "^9.6.1-alpha.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-test-renderer": "^17.0.2",
    "ts-node": "^10.4.0"
  }
